{
  "format_version": "1.21.0",
  "minecraft:entity": {
    "description": {
      "identifier": "{ENTITYIDENTIFIER}:{ENTITYNAME}",
      "is_spawnable": "{ENTITYSPAWNABLE}",
      "is_summonable": "{ENTITYSUMMONABLE}",
      "is_experimental": false
    },
    "component_groups": {
      "{ENTITYIDENTIFIER}:small_version": {
        "minecraft:movement": {
          "value": 0.2
        },
        "minecraft:flying_speed": {
          "value": 0.2
        },
        "minecraft:behavior.follow_target_captain": {
          "priority": 5,
          "speed_multiplier": 0.8,
          "within_radius": 64,
          "follow_distance": 5
        },
        "minecraft:attack": {
          "damage": 0
        },
        "minecraft:health": {
          "min": "{ENTITYHEALTHMIN}",
          "max": "{ENTITYHEALTHMAX}",
          "value": "{ENTITYHEALTHVALUE}"
        },
        "minecraft:scale": {
          "value": 0.5
        }
      },
      "{ENTITYIDENTIFIER}:large_version": {
        "minecraft:is_illager_captain": {},
        "minecraft:movement": {
          "value": 0.3
        },
        "minecraft:flying_speed": {
          "value": 0.3
        },
        "minecraft:spawn_entity": {
          "entities": [
            {
              "max_wait_time": 1,
              "min_wait_time": 1,
              "spawn_entity": "{ENTITYIDENTIFIER}:{ENTITYNAME}",
              "num_to_spawn": 2,
              "single_use": true,
              "spawn_event": "{ENTITYIDENTIFIER}:small_version"
            }
          ]
        },
        "minecraft:loot": {
          "table": "loot_tables/{ENTITYIDENTIFIER}/{ENTITYNAME}.json"
        },
        "minecraft:health": {
          "min": "{ENTITYHEALTHMIN}",
          "max": "{ENTITYHEALTHMAX}",
          "value": "{ENTITYHEALTHVALUE}"
        },
        "minecraft:attack": {
          "damage": 0
        },
        "minecraft:rideable": {
          "controlling_seat": 0,
          "crouching_skip_interact": true,
          "family_types": ["player"],
          "seat_count": 1
        }
      },
      "{ENTITYIDENTIFIER}:panic": {
        "minecraft:behavior.panic": {
          "priority": 0,
          "force": true,
          "speed_multiplier": 1.4,
          "ignore_mob_damage": false,
          "prefer_water": false
        },
        "minecraft:timer": {
          "time": [3, 5],
          "looping": false,
          "time_down_event": {
            "event": "{ENTITYIDENTIFIER}:remove_panic",
            "target": "self"
          }
        }
      }
    },
    "events": {
      "minecraft:entity_spawned": {
        "add": {
          "component_groups": [
            "{ENTITYIDENTIFIER}:large_version"
          ]
        }
      },
      "{ENTITYIDENTIFIER}:small_version": {
        "add": {
          "component_groups": [
            "{ENTITYIDENTIFIER}:small_version"
          ]
        }
      },
      "{ENTITYIDENTIFIER}:add_panic": {
        "add": {
          "component_groups": [
            "{ENTITYIDENTIFIER}:panic"
          ]
        }
      },
      "{ENTITYIDENTIFIER}:remove_panic": {
        "remove": {
          "component_groups": [
            "{ENTITYIDENTIFIER}:panic"
          ]
        }
      }
    },
    "components": {
      "minecraft:behavior.nearest_attackable_target": {
        "priority": 2,
        "entity_types": [
          {
            "filters": {
              "test": "is_family",
              "subject": "other",
              "value": "player"
            },
            "max_dist": 32
          }
        ]
      },
      "minecraft:behavior.melee_attack": {
        "priority": 1,
        "melee_fov": 90,
        "track_target": true,
        "reach_multiplier": 1.4,
        "speed_multiplier": 1.4,
        "on_attack": {
          "event": "{ENTITYIDENTIFIER}:add_panic",
          "target": "self"
        }
      },
      "minecraft:behavior.random_look_around": {
        "priority": 8
      },
      "minecraft:behavior.random_hover": {
        "priority": 12,
        "xz_dist": 8,
        "y_dist": 8,
        "y_offset": -1,
        "interval": 1,
        "hover_height": [1, 7]
      },
      "minecraft:behavior.float": {
        "priority": 19
      },
      "minecraft:type_family": {
        "family": ["{ENTITYIDENTIFIER}", "mob", "{ENTITYNAME}"]
      },
      "minecraft:breathable": {
        "total_supply": 15,
        "suffocate_time": 0
      },
      "minecraft:collision_box": {
        "width": "{ENTITYSIZEWIDTH}",
        "height": "{ENTITYSIZEHEIGHT}"
      },
      "minecraft:follow_range": {
        "value": 1024
      },
      "minecraft:damage_sensor": {
        "triggers": [
          {
            "cause": "fall",
            "deals_damage": false
          }
        ]
      },
      "minecraft:hurt_on_condition": {
        "damage_conditions": [
          {
            "filters": {
              "test": "in_lava",
              "subject": "self",
              "operator": "==",
              "value": true
            },
            "cause": "lava",
            "damage_per_tick": 4
          }
        ]
      },
      "minecraft:movement": {
        "value": 0.3
      },
      "minecraft:flying_speed": {
        "value": 0.3
      },
      "minecraft:navigation.hover": {
        "can_path_over_water": true,
        "can_sink": false,
        "can_pass_doors": false,
        "can_path_from_air": true,
        "avoid_water": true,
        "avoid_damage_blocks": true,
        "avoid_sun": false
      },
      "minecraft:movement.hover": {
        "max_turn": 6
      },
      "minecraft:jump.static": {},
      "minecraft:can_fly": {},
      "minecraft:nameable": {},
      "minecraft:physics": {},
      "minecraft:pushable": {
        "is_pushable": true,
        "is_pushable_by_piston": true
      }
    }
  }
}

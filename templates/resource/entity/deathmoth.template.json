{
    "format_version": "1.10.0",
    "minecraft:client_entity": {
        "description": {
            "identifier": "{ENTITYIDENTIFIER}:{ENTITYNAME}",
            "materials": {
                "default": "phantom"
            },
            "textures": {
                "default": "textures/{ENTITYIDENTIFIER}/entity/{ENTITYNAME}"
            },
            "geometry": {
                "default": "geometry.{ENTITYIDENTIFIER}.{ENTITYNAME}"
            },
            "render_controllers": [
                "controller.render.default"
            ],
            "spawn_egg": {
                "texture": "spawner"
            }
        }
    }
}